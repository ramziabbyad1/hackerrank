SELECT LAT_N FROM (
    SELECT @row_number:=@row_number + 1, LAT_N
    FROM STATION, (SELECT @row_number:=0) r
    ORDER BY LAT_N   
) DATA, (
    SELECT COUNT(*) AS total_count 
    FROM STATION 
);


SELECT CONCAT(NAME, '(', SUBSTR(NAME, 1, 1), ')') 
FROM OCCUPATIONS
ORDER BY NAME ASC;
SELECT CONCAT('There are a total ', COUNT(OCCUPATION) AS CNT, OCCUPATION, '.') 
FROM OCCUPATIONS
ORDER BY CNT ASC, OCCUPATION ASC;
